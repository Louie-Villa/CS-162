import numpy as np
#Use Numpy to generate a random 2-dimensional numpy array with dimensions 5x5.
array = np.random.randint(0, 11, size=(5, 5), dtype=int)
#Print the entire array.
print(array)
#Print the number from the 2nd row, 3rd column
print('The number from the 2nd row, 3rd column is: ', array[1, 2])
#Print the sum of all the elements in the array.
print('The sum of all the elements in the array is: ', np.sum(array))
#Print the mean of each row in the array.
print('The mean of each row in the array is: ', np.mean(array, axis=1))
#Print the maximum value in each column of the array.
print('The min.max of each column in the array is: ', np.max(array, axis=0))
